module untagged_noncharenc_OK {

  type record R2 {
    hexstring os
  } with {
    variant (os) "untagged";
  }

  type record R3 {
    octetstring os
  } with {
    variant (os) "untagged";
  }

  type record R4 {
    objid oid // objid doesn't have EXER
  } with {
    variant (oid) "untagged"
  }

  type boolean address;

  type record R5 {
    address a, // doesn't have EXER
    default d, // ditto
    integer last
  } with {
    variant (a) "untagged";
    variant (d) "untagged";
  }

}
with {
encode "XML";
}
