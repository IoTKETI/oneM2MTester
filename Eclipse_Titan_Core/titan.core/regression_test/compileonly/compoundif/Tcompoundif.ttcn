module Tcompoundif
{

type record comp {
  integer f1,
  integer f2
}

function func(comp p_comp) return boolean
{
  return true;
}

control
{
  if(func({1, 2})) {
    log("OK");
  }

  while(func({1, 2})) {
    log("OK");
  }

  do {
    log("OK");
  } while(func({1, 2}));

  for(var integer i:=0; func({1, i}); i:=i+1) {
    log("OK");
  }

  alt {
    [func({1, 2})] any timer.timeout {}
  }
}

}
