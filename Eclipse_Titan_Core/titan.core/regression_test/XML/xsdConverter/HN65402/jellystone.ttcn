module jellystone {
import from ttcn_ericsson_se_xsd2ttcn_HN65402 all;

type component Jelly {}

testcase tc1() runs on Jelly
{
  var RangerSmith r := {
    // Compile-time test.
    // xsd2ttcn would generate a RangerSmith record without the "food" member.
    food := "sandwiches",
    gun := omit
  }
  setverdict(pass);
}

control {
  execute(tc1());
}

}
