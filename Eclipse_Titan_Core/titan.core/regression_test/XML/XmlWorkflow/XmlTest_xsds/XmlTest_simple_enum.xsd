<schema xmlns="http://www.w3.org/2001/XMLSchema"
            xmlns:ns17="www.XmlTest.org/simple_enum"
            targetNamespace="www.XmlTest.org/simple_enum">
  <annotation>
    <documentation xml:lang="en">
      This documentum tests based on
      XML Schema Part 0: Primer Second Edition
      2.7 Building Content models
    </documentation>
  </annotation>

<simpleType name="AccountActionType">
  <restriction base="int">
    <enumeration value="0">
	    <annotation>
		    <documentation>Set</documentation>
	    </annotation>
    </enumeration>
    <enumeration value="1">
	    <annotation>
		    <documentation>Add</documentation>
	    </annotation>
    </enumeration>
    <enumeration value="2">
	    <annotation>
		    <documentation>Subtract</documentation>
	    </annotation>
    </enumeration>
    <enumeration value="3">
	    <annotation>
		    <documentation>Reset</documentation>
	    </annotation>
    </enumeration>
    <enumeration value="4">
	    <annotation>
		    <documentation>Remove</documentation>
	    </annotation>
    </enumeration>
    <enumeration value="5">
	    <annotation>
		    <documentation>Create</documentation>
	    </annotation>
    </enumeration>
  </restriction>
</simpleType>

<element name="AccountAction" type="ns17:AccountActionType"/>

</schema>
