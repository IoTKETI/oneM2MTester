        <xs:element name="Software">
          <xs:annotation>
            <xs:documentation>

	Describes the Software management service
	in the Integrated Site services Management Function Area (MFA).

            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="SoftwareInventory">
                <xs:annotation>
                  <xs:documentation>

	This class includes actions to download files and import backups
	into the software inventory.

                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Backup" minOccurs="0" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:appinfo confd:config="false"/>
                        <xs:documentation>

	An instance of a backup that is stored locally.

                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="id" type="xs:unsignedInt"                          >
                            <xs:annotation>
                              <xs:documentation>

	  The site local backup identity.

                              </xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="name" type="xs:token" minOccurs="0"                          >
                            <xs:annotation>
                              <xs:documentation>

	  A backup name selected by the user or a generated name.

                              </xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="bsRef" type="confd:objectRef" minOccurs="0"                          >
                            <xs:annotation>
                              <xs:documentation>

	  A reference to the blade system that this backup was created from.

                              </xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="date" type="xs:dateTime" minOccurs="0"                          >
                            <xs:annotation>
                              <xs:documentation>

	  The date and time when this backup was created.

                              </xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="size" type="xs:unsignedInt" minOccurs="0"                          >
                            <xs:annotation>
                              <xs:documentation>

	  The size of the backup file in kilobytes. If the backup file
	  size is smaller than 0,5 kbyte the value will be 0.

                              </xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="SoftwareAlarm" minOccurs="0" maxOccurs="unbounded">
                      <xs:annotation>
