    <xs:restriction base="xs:integer">
      <xs:maxInclusive value="31"/>
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="subrack2PortType-29">
    <xs:restriction base="xs:integer">
      <xs:maxInclusive value="6"/>
      <xs:minInclusive value="1"/>
    </xs:restriction>
  </xs:simpleType>
