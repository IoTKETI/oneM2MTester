    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="physicalAddrPlug3Type-20">
    <xs:restriction base="xs:integer">
      <xs:maxInclusive value="15"/>
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="statusType-33">
    <xs:restriction base="xs:token">
      <xs:pattern value="complete|incomplete|inconsistent"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="jobStatusType-34">
    <xs:restriction base="xs:token">
      <xs:pattern value="notStarted|ongoing|complete"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DummyEmptyType">
    <xs:restriction base="xs:token">
      <xs:enumeration value=""/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="widthType-16">
    <xs:restriction base="xs:integer">
      <xs:maxInclusive value="12"/>
      <xs:minInclusive value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="maskType-5">
    <xs:restriction base="xs:integer">
      <xs:maxInclusive value="32"/>
