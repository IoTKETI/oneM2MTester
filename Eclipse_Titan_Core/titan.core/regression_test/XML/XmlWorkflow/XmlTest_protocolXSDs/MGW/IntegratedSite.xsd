                            <xs:annotation>
                              <xs:documentation>

	The value of this attribute identifies the
	slot position within the subrack.

                              </xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="state" type="IntegratedSite:Slot_HwmSlotState" minOccurs="0"                          >
