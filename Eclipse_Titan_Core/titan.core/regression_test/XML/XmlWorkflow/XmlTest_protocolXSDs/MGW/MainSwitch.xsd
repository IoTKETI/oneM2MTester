                                  <xs:annotation>
                                    <xs:documentation>
                                      
				A 6-octet read-only value containing the individual MAC
				address assigned to the Aggregator.
              
                                    </xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="actorId" type="xs:token" minOccurs="0"                                >
                                  <xs:annotation>
                                    <xs:documentation>
                                      
				A 6-octet read-write MAC address value used as a unique
				identifier for the system that contains this Aggregator.
              
                                    </xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="actorPrio" type="xs:integer" minOccurs="0"                                >
                                  <xs:annotation>
                                    <xs:documentation>
                                      
				A 2-octet read-write value indicating the priority value
				associated with the Actors System ID.
              
                                    </xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="actorAdminKey" type="xs:integer" minOccurs="0"                                >
                                  <xs:annotation>
                                    <xs:documentation>
                                      
				The current administrative value of the Key for the
				Aggregator.
              
                                    </xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="actorOperKey" type="xs:integer" minOccurs="0"                                >
                                  <xs:annotation>
                                    <xs:documentation>
                                      
				The current operational value of the Key for the
				Aggregator.
              
                                    </xs:documentation>
                                  </xs:annotation>
                                </xs:element>
