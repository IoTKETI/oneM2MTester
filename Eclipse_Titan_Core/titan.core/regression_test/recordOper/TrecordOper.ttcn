  if(x1.x1==2) {setverdict(pass);}
  else {setverdict(fail);}
  if(x1.x2==2.2) {setverdict(pass);}
  else {setverdict(fail);}
  if(not(isbound(x1.x3))) {setverdict(pass);}
  else {setverdict(fail);}

