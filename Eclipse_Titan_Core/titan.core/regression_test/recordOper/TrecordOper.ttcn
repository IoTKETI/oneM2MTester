// restricted type: `type myrecr1 myrecr2 ({ "", "" }, { "", "" })'.
type record myrec1 {
  integer f1 (0..255),
  charstring f2,
  charstring f3 optional
}
type myrec1 myrec2 ({1, "", ""}, {2, "", ""}, {f1 := 3, f2 := "", f3 := omit})
