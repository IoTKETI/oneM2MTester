module no {

type component No {}

testcase tc_no() runs on No
{
  timer boredom := 0.3;
  boredom.start;
  boredom.timeout;
  log("nothing to see here either");
  setverdict(pass);
}

testcase tc_nooooo() runs on No
{
  timer boredom := 0.5;
  boredom.start;
  boredom.timeout;
  log("Do not want");
  setverdict(pass);
}

control {
  log("Nothing to see here");
  execute(tc_no());
  execute(tc_nooooo());
  execute(tc_no());
}

}
with {
  extension "version R2D2";
}
