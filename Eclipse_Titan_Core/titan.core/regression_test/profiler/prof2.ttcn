module prof2 {

import from prof1 all;

function f2(inout integer x) runs on C {
  if (x < 0) {
    x := x + 1;
  }
  else {
    x := x - 1;
  }
}

template integer t1(in integer x) := (0..x);

testcase tc2() runs on C {
  var integer x := 10;
  f1(x);
  f2(x);
  log(x);
  var template integer tx := t1(x);
}

control {
  execute(tc2());
}

}
