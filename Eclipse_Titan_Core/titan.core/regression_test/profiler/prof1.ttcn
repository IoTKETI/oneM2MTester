module prof1 {

type component C {}

const integer c1 := 7;

function f1(inout integer x) runs on C {
  x := x + c1;
}

testcase tc1() runs on C {
  var integer x := 6;
  f1(x);
  log(x);
  x := x + 1;
}

control {
  execute(tc1());
}

}
