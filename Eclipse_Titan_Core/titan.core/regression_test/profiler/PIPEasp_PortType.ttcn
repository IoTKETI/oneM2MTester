module PIPEasp_PortType
{

  import from PIPEasp_Types all;
// *************************************************************************
// *  PIPE port type definitions                                           *
// *************************************************************************

  // system PIPE port type
  type port PIPEasp_PT message
  {
    out ASP_PExecute,
        ASP_PExecuteBinary,
        ASP_PExecuteBackground,
        ASP_PStdin,
        ASP_PStdinBinary,
        ASP_PKill,
        ASP_PLineMode,
        ASP_PEndOfInput;

    in  ASP_PResult,
        ASP_PResultBinary,
        ASP_PStdout,
        ASP_PStderr,
        ASP_PStdoutBinary,
        ASP_PStderrBinary,
        ASP_PExit,
        ASP_PError;
  }

}//eof module
