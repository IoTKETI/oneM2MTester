module Syn_26_ModuleControl_003 {

	type component GeneralComp {}

	testcase TC_Syn_26_ModuleControl_003() runs on GeneralComp 
	{ setverdict(pass);	}

	control{
		var integer v_test := 1;
		select(v_test) {
			case(1) {
				// do something
			}
			case(2) {
				// do something else
			}
			case else {
				// do something else
			}
		}

		execute(TC_Syn_26_ModuleControl_003()); //not part of the test
	}

}
